{
  "name": "@trozzolo/gehatotal",
  "version": "1.0.0",
  "description": "Build for gehatotal.com",
  "main": "base.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=dev webpack --colors --progress",
    "build:watch": "NODE_ENV=dev webpack --colors --progress --watch",
    "build:static": "npm run build && npm run harp:compile && npm run pretty:html",
    "harp:server": "harp server --port 9876",
    "harp:compile": "harp compile",
    "browser-sync:proxy": "browser-sync start --proxy 'localhost:9876' --files 'public/**/*.jade, public/**/*.json, public/build/js/*.js, public/build/css/*.css'",
    "browser-sync:static": "browser-sync start --server 'www' --files 'www/**/*.css, www/**/*.html'",
    "pretty:html": "find _app/public/www/ -name '*.html' -exec js-beautify --html -r {} +",
    "start": "npm run build:watch",
    "deploy": "NODE_ENV=prod webpack --progress",
    "deploy:watch": "NODE_ENV=prod webpack --progress --watch"
  },
  "keywords": [
    "geha",
    "total",
    "trozzolo"
  ],
  "author": "Trozzolo Communications Group",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:JoeBartlett/GEHATotal.git"
  },
  "devDependencies": {
    "apollo.js": "github:toddmotto/apollo",
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "blazy": "^1.8.2",
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.18.6",
    "chosen-js": "^1.6.2",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "harp": "^0.23.0",
    "jade-php": "^0.1.4",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "js-beautify": "^1.6.8",
    "list.js": "^1.5.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.3.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "riot": "^3.0.7",
    "riotjs-loader": "^3.0.0",
    "sass-loader": "^4.1.1",
    "slick-carousel": "^1.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
